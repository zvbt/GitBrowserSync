{
  "manifest_version": 3,
  "name": "GitBrowserSync",
  "description": "Sync bookmarks & extensions to a github repo.",
  "author": "zvbt",
  "homepage_url": "https://github.com/zvbt/GitBrowserSync",
  "version": "0.0.1",
  "permissions": [
    "bookmarks",
    "management",
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
